import java.util.*;
public class ProcessingNumbers {
	
	public static void main (String[]args){
		Scanner firstInput = new Scanner(System.in);
		System.out.println("Please enter your first value");
		double val1 = firstInput.nextInt();
		Scanner secondInput = new Scanner (System.in);
		System.out.println("Please enter your second value");
		double val2 = secondInput.nextInt();
		if(val1%2 == 0 && val2%2 == 0){
			System.out.print("Your numbers are good.");
		}
		if (val1>val2){
			System.out.println(Math.abs(val1));
		}else{
			System.out.println(Math.abs(val2));
		}
	}

}
