import java.util.*;
public class ProcessingNumbers {
	
	public static void main (String[]args){
		Scanner firstInput = new Scanner(System.in);
		System.out.println("Please enter your first value");
		int val1 = firstInput.nextInt();
		Scanner secondInput = new Scanner (System.in);
		System.out.println("Please enter your second value");
		int val2 = secondInput.nextInt();
		if(val1%2 == 0 && val2%2 == 0){
			System.out.print(val1);
			System.out.println(val2);
		}else{
			System.out.println("Your numbers are not even.");
		}
		if (val1>val2){
			System.out.println(val1);
		}else{
			System.out.println(val2);
		}
	}

}
